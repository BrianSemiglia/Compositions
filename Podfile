# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Compositions' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Compositions
  pod 'RxSwift'
  pod 'RxCocoa'
  pod "RxGesture", :podspec => "https://gist.githubusercontent.com/trispo/29ec5dfefca0ecf8a57fc2f3fc3c4956/raw/bf3f85d6762405e98a638080909d19cb65c35f61/RxGesture.podspec"
  pod 'RxCallbacks'
  pod 'RxAlamofire'
  pod 'Differ'

  target 'CompositionsTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
